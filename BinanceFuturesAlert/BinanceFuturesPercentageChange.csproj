<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows10.0.17763.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <AssemblyName>Futuros Binance - Percentage Change</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\down-arrow.png" />
    <None Remove="Images\loading.gif" />
    <None Remove="Images\up-arrow.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="WpfAnimatedGif" Version="2.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\down-arrow.png" />
    <Resource Include="Images\loading.gif" />
    <Resource Include="Images\up-arrow.png" />
  </ItemGroup>

</Project>
